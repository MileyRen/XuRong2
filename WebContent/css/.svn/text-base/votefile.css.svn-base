@CHARSET "UTF-8";

	#container {
		margin: auto;
		margin-top: -15px;
		position: relative;
		border: none;
		font-family:"方正粗圆简体";font-size: 30px;
		line-height:60px;
		margin-top:0; margin-bottom:0; margin-left:0; margin-right:0;
		border-width:0;
		padding: 0px;
		color: white;
	}
	.myback{
		BACKGROUND-COLOR: transparent;padding: 0;
		font-family:"方正粗圆简体";font-size: 30px;
		margin-top:0; margin-bottom:0; margin-left:0; margin-right:0;
		border-width:0;
		line-height:60px;
		padding: 0px;
		color: white;
	}
	.tableline{
		line-height: 60px;
	}
	label{width:47px; height:46px; float:left; display:inline; margin-right:5px; position:relative; overflow:hidden;}
	label input{margin-left:-999em; float:left;}

	label.hover .oncheck{display:block; width:47px; height:46px; background:url(../../images/checked.png) no-repeat 0 0; position:absolute; left:0; top:0;}

	